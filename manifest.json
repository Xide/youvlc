{
  "manifest_version": 2,
  "name": "youvlc",
  "description": "open your Youtube videos with VLC",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "nativeMessaging",
    "storage"
  ],
  "background": {
    "scripts": [
      "youvlc.js"
    ]
  },
  "options_page": "settings/index.html",
  "options_ui": {
    "page": "settings/index.html",
    "chrome_style": true
  },
  "icons": {
    "16": "icons/small.png"
  }
}       
